<?xml version='1.0' encoding='utf-8'?>
<odoo>
     <record model="ir.module.category" id="onyx_social_media">
        <field name="name">onyx media</field>
        <field name="description">Onyx Media</field>
        <field name="sequence">17</field>
    </record>

    <record id="group_onyx_user" model="res.groups">
        <field name="name">Onyx Poster</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="category_id" ref="onyx_social_media"/>
    </record>

    <record id="group_onyx_manager" model="res.groups">
        <field name="name">Onyx Manager</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        <field name="category_id" ref="onyx_social_media"/>
        <field name="implied_ids" eval="[(4, ref('group_onyx_user'))]"/>
    </record>

</odoo>