<odoo>
  <template id="revision_request" name="Revision Request">
    <t t-name="SocialCalendar">
      <t t-call="website.layout">
        <div class="row container">
          <div class="container-fluid" style="display: inherit;">
            <div class="col-md-8" style="float: left; width: 100% !important; height: 1000px; left: -324px !important;margin-left: 7px;">
              <div class="col-md-12 bg-100 roundest-border table-1 py-5 text-center" style="box-shadow: 0px 0px 19px rgba(0, 0.05, 0.05, 0.12) inset;
                                   height: 174px !important; text-align: start !important; overflow-y: auto;
                                   border-radius: 45px; padding: 20px; width: 100% !important; height: 50% !important;
                                    margin-top: 50px;">
                <div class="text-center">
                        <i style="color: #00d3a1 !important; font-size:75px !important" class='fa fa-bell'></i><br/><br/>
                  <strong style="font-weight: bolder !important; font-size: 36px !important;
                                           font-family: Oxygen, Ubuntu, Cantarell, 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif !important;" class="text-muted">REVISION REQUESTS</strong>
                </div>
                <t t-foreach="posts" t-as="val">
                  <div class="row" style="margin-top: 46px;">
                    <ul class="checkmark">
                      <div class="circles"><t t-esc="val['count']"/></div>
                    </ul>
                    <ul class="checkmarks">
                      <li>
                        <div class="col-12">
                          <div class="feedback-table-1" style="font-size: 20px !important;">
                            <t t-esc="val['name']"/>&amp;nbsp; <strong class="text-muted">provided Feedback! &amp;nbsp; <a href="/content_calendar">View Feedback</a>
                            </strong>
                          </div>
                        </div>
                      </li>
                    </ul>
<!--                    <span t-esc="posts"/>-->
<!--                    <t t-esc="posts"/>-->
                  </div>
                </t>
              </div>
            </div>
            <head>
              <meta charset="utf-8" />
              <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
              <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css" />
              <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet" />
              <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
              <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
              <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css'/>
            </head>
            <body>
              <div class="mobile-header">
                <div class="row">
                  <div class="col-2">
                    <a href="#" data-activates="sidebar" class="button-collapse" style="">
                      <i class="material-icons">menu</i>
                    </a>
                  </div>
                  <div class="col">
                    <h4>Events</h4>
                  </div>
                </div>
              </div>
              <div class="col-10" style="margin-top: 45px;margin-left: -140px;padding-right: 114px;">
                <div class="col-10">
                  <div class="header-background">
                    <div class="calendar-header">
                      <a style="margin-top: -14px !important;" class="prev-button" id="prev">
                        <i class="material-icons">keyboard_arrow_left </i>
                      </a>
                      <a style="margin-top: -14px !important;" class="next-button" id="next">
                        <i class="material-icons">keyboard_arrow_right</i>
                      </a>
                      <div class="row header-title">
                        <div class="header-text">
                          <h3 style="cursor: default !important; text-transform: uppercase !important;
                                                                        font-family: Oxygen, Ubuntu, Cantarell, 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif !important;
                                                                         font-size: 37px !important; margin-left: 160px !important; margin-top: -0.54rem !important; font-weight: bold !important;" id="month-name">February </h3>
                          <h5 style="cursor: default !important; text-transform: uppercase !important;
                                                                         font-family: Oxygen, Ubuntu, Cantarell, 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif !important;
                                                                         font-size: 14px !important; margin-left: 160px; font-weight: bold !important;" id="todayDayName">Today is Friday 7 Feb </h5>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="calendar-content text-muted" style="padding-right: 8%;padding-left: 6%; font-size: 26px !important;">
                    <div id="calendar-table" class="calendar-cells">
                      <div id="table-header">
                        <div class="row text-muted" style="text-transform: uppercase !important; font-weight: lighter !important;
                                                                                            font-family: Oxygen, Ubuntu, Cantarell, 'Fira Sans',
                                                                                            'Droid Sans', 'Helvetica Neue', sans-serif !important;">
                          <div class="col">Mon</div>
                          <div class="col">Tue</div>
                          <div class="col">Wed</div>
                          <div class="col">Thu</div>
                          <div class="col">Fri</div>
                          <div class="col">Sat</div>
                          <div class="col">Sun</div>
                        </div>
                      </div>
                      <div id="table-body" class="" style="font-family: Oxygen, Ubuntu, Cantarell, 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif;"></div>
                    </div>
                  </div>
                  <div class="sidebar-wrapper" id="sidebar" style="color: black !important;">
                    <div class="sidebar-title">
                      <h5 id="eventDayName"></h5>
                    </div>

                    <div class="sidebar-events" id="sidebarEvents" style="font-family: Oxygen, Ubuntu, Cantarell, 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif; font-weight: bolder! important;
                                                         margin-left: -61px !important; padding-left: 13% !important; color: #fff !important">
                      <div class="empty-message">Sorry, no clients to selected date </div>
                    </div>
                  </div>
              <div class="calendar-footer-1">
              <div class="emptyForm" id="emptyForm">
                <h4 id="emptyFormTitle">No Clients Added Now</h4>
                <a class="addEvent" id="changeFormButton">Add new</a>
              </div>
              <div class="addForm" id="addForm">
                <h4>Add new client</h4>

                <div class="row">
                  <div class="input-field col s6">
                    <input id="eventTitleInput" type="text" class="validate"/>
                    <label style="color: #000 !important; font-size: larger !important;" for="eventTitleInput">Client Name</label>
                  </div>
                  <div class="input-field col s6" >
                    <input style="display: none !important;" id="eventDescInput" type="text" class="validate"/>
<!--                    <label for="eventDescInput">Description</label>-->
                  </div>
                </div>

                <div class="addEventButtons" style="margin-right: 45px;">
                  <a style="  background-color: #fff !important;" class="waves-effect waves-light btn blue lighten-2" id="addEventButton">Add</a>
                  <a style="padding-right: 5rem; background-color: #fff !important;" class="waves-effect waves-light btn grey lighten-2" id="cancelAdd">Cancel</a>
                </div>

              </div>
            </div>
                </div>
              </div>
              <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js" integrity="sha384-vFJXuSJphROIrBnz7yo7oB41mKfc8JzQZiCq4NCceLEaO4IHwicKwpJf9c9IpFgh" crossorigin="anonymous"></script>
              <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js" integrity="sha384-alpBpkh1PFOepccYVYDB4do5UnbKysX5WZXm3XxPqe5iKTfUKjNkCk9SaVuEZflJ" crossorigin="anonymous"></script>
              <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
              <script>
                $(".button-collapse").sideNav();
              </script>
            </body>
          </div>
        </div>
      </t>
    </t>
  </template>
</odoo>