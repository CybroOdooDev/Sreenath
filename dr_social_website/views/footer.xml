<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="template_footer_style_11" inherit_id="website.layout" name="Footer Style - 11" active="False">
        <xpath expr="//div[@id='footer']" position="replace">
        <div id="footer" class="oe_structure oe_structure_solo" t-ignore="true" t-if="not no_footer">
            <section class="s_text_block pt40 pb16" style="rgb(0, 211, 161) !important;" data-snippet="s_text_block" data-name="Text">
                <div class="container" >
                    <div class="row">
                        <div class="col-lg-12 pt24 pb24 justify-content-center text-center">
                                COPYRIGHTS 2022 | ONYXMEDIA |
                                <a href="https://www.onyxmedia.io/terms-of-service" target="_parent" style="color: rgb(255, 255, 255);">TERMS</a>
                                &amp;
                                <a href="https://www.onyxmedia.io/privacypolicy" target="_parent" style="color: rgb(255, 255, 255);">PRIVACY POLICY</a>
                        </div>
                    </div>
                </div>
            </section>
        </div>
        </xpath>
        <xpath expr="//div[hasclass('o_footer_copyright')]/div[hasclass('container')]" position="replace"/>
    </template>

</odoo>
